#!/usr/bin/env bash
#Script that displays numbers from 1 to 20 and  Superstitious numbers 4,9 and 17.
i=1
while [ $i -lt 21 ]; do
    case $i in
	4) echo $i
	   echo "bad luck from China"
	   ;;
	9) echo $i
	   echo "bad luck from Japan"
	   ;;
	17) echo $i
	    echo "bad luck from Italy"
	*)
	    echo $i
    esac
	  ((i++)); done  
